<ViewObject
   Name="UserEmployeeDetailsVO"
   BindingStyle="OracleName"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="abc.oracle.apps.fnd.server.UserEmployeeDetailsVOImpl"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
select fu.user_id
      ,fu.user_name
	  ,ppx.full_name
	  ,ppx.email_address
	  ,ppx.nationality
  from fnd_user fu
      ,per_people_x ppx
 where 1=1
   and fu.employee_id = ppx.person_id
   and fu.user_id = 1318
   ]]></SQLQuery>
